<link rel="stylesheet" href="./css/course/course.css" />
<div class="courses">
  <div class="tab">
    <% if (categories.length > 0) { %> <% categories.forEach(category => { %>
    <button
      class="tablinks"
      onclick="openCity(event, 'Paris')"
    >
      <%= category %>
    </button>
    <% }) %> <% } %>
  </div>
  <% if (courses.length > 0) { %>
    <% courses.map(course => { 
      const {name, tutor, price, tag} = course;
      %>
       <h1><%= name %></h1>
       <p><%= tutor.tutorName %></p>
       <p><%= price %></p>
       <p><%= tag %></p>
    <% }) %>
   <% } %>
</div>
<script src="./script/script.js"></script>
